{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "64": "icon.png",
      "128": "icon.png" },

  "chrome_url_overrides":{
    "newtab": "extension.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["time_tracker.js"]
    }
  ],

  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "storage",
    "contextMenus",
    "http://localhost:3000/"
  ]
}
