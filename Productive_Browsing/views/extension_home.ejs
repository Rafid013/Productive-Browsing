<!doctype html>

<html>
<head>
    <title><%= title %></title>
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.2/semantic.min.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/extension_home_css.css">
</head>

<body id="homepage_body">
<div id="MyClockDisplay" class="clock" onclick="toggle()"></div>
<marquee id="ShowEventScroll" behavior="scroll" direction="left" scrollamount="7"></marquee>
<script type="text/javascript" src="/javascripts/extension_home_js.js">
</script>
<div class="change_background">
    <div hidden="true"><input type="file" id="finput" accept=".gif,.jpg,.jpeg,.png" onchange="fileInput()"></div>
    <i class="large write square icon" onclick="selectBackground()"></i>
</div>
<div id="Things_To_DO">Stuffs to do Today</div>
<form id="Task_Input" class="ui form" onsubmit="add_new_task()">
    <input type="text" name="Text" value="" placeholder="To Do">
    <div class="two fields" >
        <input type="date" name="Date">
        <input type="time" name="Time">
    </div>
    <input id="input_submit" type="submit" name="Submit" value="ADD">
</form>
</body>
</html>



